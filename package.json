{
  "name": "bunny-generator",
  "version": "1.0.0",
  "description": "A simple GitHub app to spam cute pictures on merged PullRequests",
  "author": "Hubert Bere≈õ <h.bere@warwick.ac.uk> (https://github.com/Ddedalus)",
  "license": "ISC",
  "repository": "https://github.com//bunny-generator.git",
  "homepage": "https://github.com//bunny-generator",
  "bugs": "https://github.com//bunny-generator/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "@chadfawcett/probot-serverless-now": "^1.0.0",
    "acorn": "^6.4.1",
    "handlebars": "^4.7.3",
    "probot": "^7.2.0",
    "yourself": "^1.0.0"
  },
  "devDependencies": {
    "jest": "^24.9.0",
    "nock": "^10.0.0",
    "nodemon": "^1.19.4",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
